{
  "name": "@captify-io/pmbook",
  "version": "1.0.17",
  "description": "Strategic alignment and business operations platform for government contracting",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.18 || >=20"
  },
  "packageManager": "npm@9.8.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": {
        "import": "./dist/index.ts",
        "types": "./dist/index.ts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./app": {
      "development": {
        "import": "./dist/app/index.ts",
        "types": "./dist/app/index.ts"
      },
      "import": "./dist/app/index.js",
      "require": "./dist/app/index.cjs",
      "types": "./dist/app/index.d.ts"
    },
    "./components": {
      "development": {
        "import": "./dist/components/index.ts",
        "types": "./dist/components/index.ts"
      },
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs",
      "types": "./dist/components/index.d.ts"
    },
    "./services": {
      "development": {
        "import": "./dist/services/index.ts",
        "types": "./dist/services/index.ts"
      },
      "import": "./dist/services/index.js",
      "require": "./dist/services/index.cjs",
      "types": "./dist/services/index.d.ts"
    },
    "./types": {
      "development": {
        "import": "./dist/types/index.ts",
        "types": "./dist/types/index.ts"
      },
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./config": {
      "development": {
        "import": "./dist/config.ts",
        "types": "./dist/config.ts"
      },
      "import": "./dist/config.js",
      "require": "./dist/config.cjs",
      "types": "./dist/config.d.ts"
    }
  },
  "scripts": {
    "dev": "next dev -p 5001 --turbo",
    "build": "next build ",
    "start": "next start",
    "dist": "tsup && tsc --project tsconfig.build.json --emitDeclarationOnly",
    "push": "npm run build && npm run version:patch && git add -A && git commit -m 'chore: release v$npm_package_version' && git push origin main",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist",
    "turbo:dev": "turbo dev",
    "turbo:build": "turbo build",
    "turbo:lint": "turbo lint",
    "turbo:type-check": "turbo type-check",
    "turbo:clean": "turbo clean"
  },
  "dependencies": {
    "@captify-io/platform": "^1.0.59",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.3.1",
    "@types/react": "19.1.1",
    "@types/react-dom": "19.1.1",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.5.2",
    "next": "^15.5.2",
    "next-auth": "5.0.0-beta.25",
    "next-themes": "^0.4.0",
    "postcss": "^8.4.0",
    "tailwind-merge": "^3.0.0",
    "tailwindcss": "^4.1.13",
    "react": ">=19.1.1",
    "react-dom": ">=19.1.1",
    "lucide-react": "^0.544.0",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "turbo": "^2.5.6",
    "tsup": "^8.5.0",
    "typescript": "5.9.2",
    "uuid": "^11.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/captify-io/pmbook.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "author": "Captify IO",
  "license": "MIT",
  "homepage": "https://github.com/captify-io/pmbook#readme",
  "bugs": {
    "url": "https://github.com/captify-io/pmbook/issues"
  }
}
