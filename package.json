{
    "name": "@captify-io/pmbook",
    "version": "1.0.4",
    "description": "Strategic alignment and business operations platform for government contracting",
    "type": "module",
    "sideEffects": [
        "**/*.css"
    ],
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=18.18 || >=20"
    },
    "main": "dist/app.mjs",
    "module": "dist/app.mjs",
    "exports": {
        ".": {
            "import": "./dist/app.mjs"
        },
        "./app": {
            "import": "./dist/app.mjs"
        },
        "./components": {
            "import": "./dist/components.mjs"
        },
        "./hooks": {
            "import": "./dist/hooks.mjs"
        },
        "./lib": {
            "import": "./dist/lib.mjs"
        },
        "./services": {
            "import": "./dist/services.mjs"
        },
        "./types": {
            "import": "./dist/types.mjs"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "cross-env NODE_ENV=development tsup --watch",
        "type-check": "npx tsc --noEmit",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.693.0",
        "@aws-sdk/client-s3": "^3.693.0",
        "@aws-sdk/lib-dynamodb": "^3.693.0",
        "date-fns": "^3.0.0",
        "lucide-react": "^0.542.0",
        "recharts": "^2.12.7",
        "resolve-from": "^5.0.0",
        "rimraf": "^6.0.1",
        "uuid": "^11.1.0"
    },
    "peerDependencies": {
        "@captify-io/core": ">=1.0.0",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/captify-io/pmbook.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com",
        "access": "public"
    },
    "devDependencies": {
        "@types/node": "^24.3.1",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "cross-env": "^7.0.3",
        "esbuild": "0.25.8",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    },
    "module": "dist/app.mjs",
    "author": "Captify IO",
    "license": "MIT",
    "homepage": "https://github.com/captify-io/pmbook#readme",
    "bugs": {
        "url": "https://github.com/captify-io/pmbook/issues"
    }
}
